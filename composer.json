{
    "name": "xamani/nova-persian-number",
    "description": "Laravel Nova Number field with display of Persian letters.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Xamani\\NovaPersianNumber\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xamani\\NovaPersianNumber\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
